{
	"DSP_*.bin":{
		"type":"must",
                "path":"bin/",
		"rename":"dsp.bin"
	},
	"*MDBIN*.bin":{
                "type":"must",
                "path":"bin/",
		"rename":"modem.img"
	},
	"MDDB*.EDB":{
                "type":"must",
                "path":"dhl/database/",
                "multiple":"yes",
		"remove":"yes"
        },
	"MDDB.META.ODB_*.XML.GZ":{
                "type":"must",
                "path":"dhl/database/",
		"remove":"yes"
        },
	"catcher_filter.bin":{
                "type":"must",
                "path":"dhl/database/"
        },
        "catcher_filter_PLS_PS_ONLY.bin":{
                "type":"must",
                "path":"dhl/database/"
        },
        "MDDB.MCF.ODB.tar.gz":{
                "type":"must",
                "path":"bin/"
        },
        "MCF_OTA*_FILES.tar.gz":{
                "type":"must",
                "path":"bin/",
                "desc":"support MCF 2.0 and 3.0 main OTA IMG"
        },
        "MCF_OTA_IMG_FILES_*.tar.gz":{
                "type":"must",
                "path":"bin/",
                "multiple":"yes",
                "allow_zero_match":"yes",
                "desc":"support MCF 3.0 misc OTA IMG"
        },
        "catcher_filter_1_Moderate.bin":{
                "type":"must",
                "path":"dhl/database/"
        },
        "catcher_filter_2_Standard.bin":{
                "type":"must",
                "path":"dhl/database/"
        },
        "catcher_filter_3_Slim.bin":{
                "type":"must",
                "path":"dhl/database/"
        },
        "catcher_filter_4_UltraSlim.bin":{
                "type":"must",
                "path":"dhl/database/"
        },
        "catcher_filter_LowPowerMonitor.bin":{
                "type":"must",
                "path":"dhl/database/"
        },
        "mddata":{
                "type":"must",
                "path":""
        },
        "md_version_info.log":{
                "type":"release"
        },
        "mipc.release.tar.gz":{
                "type":"must",
                "path":"modem/mipc/"
        }
}
